{
    "name": "symfona/api-specification",
    "type": "library",
    "description": "PHP API Specification",
    "keywords": ["php", "api", "specification"],
    "homepage": "https://github.com/symfona/api-specification",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symfona\\Specification\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfona\\Specification\\Api\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
